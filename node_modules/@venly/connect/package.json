  "description": "Venly Connect SDK",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "unpkg": "dist/connect.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build-ts": "tsc -p .",
    "build-js": "npx webpack --mode production",
