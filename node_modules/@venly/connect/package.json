  "bugs": {
    "url": "https://github.com/ArkaneNetwork/arkane-connect/issues"
  },
  "homepage": "https://venly.io",
  "dependencies": {
    "@types/node": "^12.7.1",
    "axios": "^0.21.1",
    "core-js": "^3.2.1",
